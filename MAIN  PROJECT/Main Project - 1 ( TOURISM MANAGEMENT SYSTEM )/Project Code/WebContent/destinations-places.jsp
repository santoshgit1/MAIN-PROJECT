	<div class="gallery py-5">
		<div class="container-fluid">
			<h2 class="heading text-center mb-sm-5 mb-4">Destination Places</h2>
			<div class="row">
				<div class="col-sm-12">
					<table class="table table-bordered">
						<thead>
							<tr>
								<th>Place Name</th>
								<th>Place</th>
								<th>Address</th>
								<th>Area</th>
								<th>Tags</th>
								<th>Place Description</th>
							</tr>
						</thead>
						<%
							ResultSet resultset = DatabaseConnection.getResultFromSqlQuery("select * from tblplaces");
							while (resultset.next()) {
						%>
						<tbody>
							<tr>
								<td><%=resultset.getString(2)%></td>
								<td><image src="upload/<%=resultset.getString("image_name")%>" style="width:200px;height:150px;"></image></td>
								<td><%=resultset.getString(5)%></td>
								<td><%=resultset.getString(6)%></td>
								<td><%=resultset.getString(7)%></td>
								<td><%=resultset.getString(8)%></td>
							</tr>
						</tbody>
						<%
							}
						%>
					</table>
				</div>
			</div>
		</div>
	</div>
